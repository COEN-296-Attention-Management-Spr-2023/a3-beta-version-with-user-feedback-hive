{
  "name": "Todo Timer",
  "version": "1.2.4",
  "manifest_version": 2,

  "description": "A3 Hive project",
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "img/clock.png",
    "default_popup": "popup.html"
  },

  "offline_enabled": true,

  "permissions": [
    "notifications"
  ],

  "web_accessible_resources": [
    "img/32.png"
  ]
}
